{
  "name": "Djoe screen capture extension",
  "author": "RÃ©mi Passmoilesel",
  "version": "1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "description": "Capture screen for Djoe !",
  "homepage_url": "https://github.com/remipassmoilesel/djoe",
  "background": {
    "scripts": [
      "background-script.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "all_frames": true,
      "run_at": "document_end",
      "matches": [
        "https://127.0.0.1/*",
        "https://localhost/*",
        "https://192.168.1.36/*",
        "https://im.silverpeas.net/*"
      ]
    }
  ],
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}